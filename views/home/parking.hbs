    flex-wrap: nowrap;
    justify-content: center;
    align-items: stretch;
    align-content: stretch;

}
.containerparking .col-6 .grids .col:after
{
    content: "";
    height: 5px;
    width: 100%;
    background: black;
    position: absolute;
    /* display: block; */
    bottom: 0;
    left: 0;
    right: 0;
}
.containerparking .col-6 .grids:last-child .col:after
{
    content: "";
    display: none;
}
.containerparking .col-6 .col:last-child{
    border-right: 2px solid black;
}

